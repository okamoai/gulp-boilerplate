{
  "name": "gulp-boilerplate",
  "version": "1.4.0",
  "description": "Static web page boilerplate with gulp.js",
  "main": "gulpfile.babel.js",
  "scripts": {
    "clean": "gulp clean",
    "clean:dev": "cross-env NODE_ENV=development npm run clean",
    "clean:stg": "cross-env NODE_ENV=stage npm run clean",
    "clean:prd": "cross-env NODE_ENV=production npm run clean",
    "copy": "gulp copy",
    "copy:dev": "cross-env NODE_ENV=development npm run copy",
    "copy:stg": "cross-env NODE_ENV=stage npm run copy",
    "copy:prd": "cross-env NODE_ENV=production npm run copy",
    "sass": "gulp sass",
    "sass:dev": "cross-env NODE_ENV=development npm run sass",
    "sass:stg": "cross-env NODE_ENV=stage npm run sass",
    "sass:prd": "cross-env NODE_ENV=production npm run sass",
    "postcss": "gulp postcss",
    "postcss:dev": "cross-env NODE_ENV=development npm run postcss",
    "postcss:stg": "cross-env NODE_ENV=stage npm run postcss",
    "postcss:prd": "cross-env NODE_ENV=production npm run postcss",
    "font": "gulp font",
    "font:dev": "cross-env NODE_ENV=development npm run font",
    "font:stg": "cross-env NODE_ENV=stage npm run font",
    "font:prd": "cross-env NODE_ENV=production npm run font",
    "imagemin": "gulp imagemin",
    "imagemin:dev": "cross-env NODE_ENV=development npm run imagemin",
    "imagemin:stg": "cross-env NODE_ENV=stage npm run imagemin",
    "imagemin:prd": "cross-env NODE_ENV=production npm run imagemin",
    "js": "gulp js",
    "js:dev": "cross-env NODE_ENV=development npm run js",
    "js:stg": "cross-env NODE_ENV=stage npm run js",
    "js:prd": "cross-env NODE_ENV=production npm run js",
    "jsConcat": "gulp jsConcat",
    "jsConcat:dev": "cross-env NODE_ENV=development npm run jsConcat",
    "jsConcat:stg": "cross-env NODE_ENV=stage npm run jsConcat",
    "jsConcat:prd": "cross-env NODE_ENV=production npm run jsConcat",
    "pug": "gulp pug",
    "pug:dev": "cross-env NODE_ENV=development npm run pug",
    "pug:stg": "cross-env NODE_ENV=stage npm run pug",
    "pug:prd": "cross-env NODE_ENV=production npm run pug",
    "ejs": "gulp ejs",
    "ejs:dev": "cross-env NODE_ENV=development npm run ejs",
    "ejs:stg": "cross-env NODE_ENV=stage npm run ejs",
    "ejs:prd": "cross-env NODE_ENV=production npm run ejs",
    "sprite": "gulp sprite",
    "sprite:dev": "cross-env NODE_ENV=development npm run sprite",
    "sprite:stg": "cross-env NODE_ENV=stage npm run sprite",
    "sprite:prd": "cross-env NODE_ENV=production npm run sprite",
    "build": "gulp build",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:stg": "cross-env NODE_ENV=stage npm run build",
    "build:prd": "cross-env NODE_ENV=production npm run build",
    "eslint": "gulp eslint",
    "watch": "gulp watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "okamoai <okamoai@rano-raraku.net>",
  "license": "MIT",
  "engines": {
    "node": "~10.14.1",
    "npm": "~6.4.1"
  },
  "browserslist": [
    "ie 11",
    "last 1 Edge versions",
    "last 1 Firefox versions",
    "last 1 Chrome versions",
    "last 1 Safari versions",
    "last 2 iOS versions",
    "Android >= 4.4",
    "last 1 ChromeAndroid versions"
  ],
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "config": "^3.0.0",
    "crc": "^3.8.0",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.7",
    "del": "^3.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-util": "^0.5.5",
    "gulp-consolidate": "^0.2.0",
    "gulp-data": "^1.3.1",
    "gulp-debug": "^4.0.0",
    "gulp-ejs": "^3.3.0",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-iconfont": "^10.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp.spritesmith": "^6.9.0",
    "husky": "^1.2.0",
    "imagemin-pngquant": "^6.0.0",
    "lint-staged": "^8.1.0",
    "mkdirp": "^0.5.1",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-partial-import": "^4.1.0",
    "prettier-eslint-cli": "^4.7.1",
    "replacestream": "^4.0.3",
    "through2": "^3.0.0",
    "vinyl-buffer": "^1.0.1",
    "webpack": "^4.27.0",
    "webpack-stream": "~5.0.0"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "jquery.easing": "^1.3.2",
    "lodash": "^4.17.10"
  }
}
